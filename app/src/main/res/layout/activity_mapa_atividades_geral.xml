<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MapaAtividadesGeral">

    <ImageView
        android:id="@+id/imgMap5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/cor_fundo_rosa"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/mapinha22" />

    <Button
        android:id="@+id/btProsseguir5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        android:background="@color/statusBarRoxo"
        android:fontFamily="@font/londrina_solid"
        android:text="Continuar"
        android:textColor="@android:color/white"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.244"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:fontFamily="@font/londrina_solid"
        android:text="Mapa de Progresso"
        android:textColor="@android:color/white"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:fontFamily="@font/londrina_solid"
        android:text="Pontos"
        android:textSize="30sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/txtTotalPontos4"
        app:layout_constraintStart_toEndOf="@+id/txtTotalPontos4" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:fontFamily="@font/londrina_solid"
        android:text="Retornos"
        android:textSize="14sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/txtQtdVoltou"
        app:layout_constraintStart_toEndOf="@+id/txtQtdVoltou" />

    <TextView
        android:id="@+id/txtTotalPontos4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="496dp"
        android:fontFamily="@font/londrina_solid"
        android:text="Total:"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtQtdVoltou"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="16dp"
        android:fontFamily="@font/londrina_solid"
        android:text="Total :"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/btProsseguir5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtTotalPontos4" />

    <ImageView
        android:id="@+id/imgStar"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        app:srcCompat="@android:drawable/btn_star_big_on" />

    <ImageView
        android:id="@+id/imgQtdVidas"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="32dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <ImageView
        android:id="@+id/imgB1"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="160dp"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/robo3" />

    <ImageView
        android:id="@+id/imgB2"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgB1"
        app:srcCompat="@drawable/robo3" />

    <ImageView
        android:id="@+id/imgB3"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="32dp"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgB2"
        app:srcCompat="@drawable/robo3" />

    <ImageView
        android:id="@+id/imgB4"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="36dp"
        android:visibility="invisible"
        app:layout_constraintStart_toEndOf="@+id/imgB3"
        app:layout_constraintTop_toBottomOf="@+id/imgB2"
        app:srcCompat="@drawable/robo3" />

    <ImageView
        android:id="@+id/imgB5"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="76dp"
        android:visibility="invisible"
        app:layout_constraintStart_toEndOf="@+id/imgB2"
        app:layout_constraintTop_toBottomOf="@+id/imgB1"
        app:srcCompat="@drawable/robo3" />

    <ImageView
        android:id="@+id/imgB6"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="132dp"
        android:layout_marginTop="40dp"
        android:visibility="invisible"
        app:layout_constraintStart_toEndOf="@+id/imgB1"
        app:layout_constraintTop_toBottomOf="@+id/imgQtdVidas"
        app:srcCompat="@drawable/robo3" />

    <ImageView
        android:id="@+id/imgB7"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="92dp"
        android:layout_marginTop="84dp"
        android:visibility="invisible"
        app:layout_constraintStart_toEndOf="@+id/imgB5"
        app:layout_constraintTop_toBottomOf="@+id/imgQtdVidas"
        app:srcCompat="@drawable/robo3" />

    <ImageView
        android:id="@+id/imgB8"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="44dp"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgB7"
        app:srcCompat="@drawable/robo3" />

    <ImageView
        android:id="@+id/imgB9"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginTop="132dp"
        android:layout_marginEnd="116dp"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgB6"
        app:srcCompat="@drawable/robo3" />

    <ImageView
        android:id="@+id/imgB10"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="116dp"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgB9"
        app:srcCompat="@drawable/robo3" />

</android.support.constraint.ConstraintLayout>